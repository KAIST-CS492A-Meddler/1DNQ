<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.user.wase">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name="com.example.user.wase.View.SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name="com.example.user.wase.View.MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
        </activity>

        <activity android:name="com.example.user.wase.View.MyEquipmentsActivity"/>
        <activity android:name="com.example.user.wase.View.MyRecordsActivity"/>
        <activity android:name="com.example.user.wase.View.MyRoutinesActivity"/>
        <activity android:name="com.example.user.wase.View.SettingAppInfoActivity"/>
        <activity android:name="com.example.user.wase.View.SettingMyInfoActivity"/>
        <activity android:name="com.example.user.wase.View.SupportAppInfoActivity"/>
        <activity android:name="com.example.user.wase.View.SupportHelpActivity"/>

    </application>

</manifest>
