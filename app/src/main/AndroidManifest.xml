        <activity android:name=".viewtemp.activity.MyEquipmentsActivityTemp"/>
        <activity android:name=".viewtemp.activity.MyRecordsActivityTemp"/>
        <activity android:name=".viewtemp.activity.MyRoutinesActivityTemp"/>
